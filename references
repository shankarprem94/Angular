------------------------------AppModule-----------------------
entryComponents:[CartButtonComponent, CartComponent,AddressComponent,CheckoutProductsComponent,CheckoutButtonComponent],
  bootstrap: [AppComponent]
})
export class AppModule { 
  constructor(private injector: Injector) {
    
    const cartButtonComponent = createCustomElement(CartButtonComponent, { injector: this.injector });
    customElements.define('app-cart-button-qty', cartButtonComponent);

    const cartComponent = createCustomElement(CartComponent, { injector: this.injector });
    customElements.define('app-cart', cartComponent);

    // const addressComponent = createCustomElement(AddressComponent, { injector: this.injector });
    // customElements.define('app-address', addressComponent);

    const reviewOrderComponent = createCustomElement(CheckoutProductsComponent, { injector: this.injector });
    customElements.define('app-checkout-products', reviewOrderComponent);

    const addressWrapperComponent = createCustomElement(AddressComponent, { injector: this.injector });
    customElements.define('app-address', addressWrapperComponent);

    const checkoutButtonComponent = createCustomElement(CheckoutButtonComponent, { injector: this.injector });
    customElements.define('app-checkout-button', checkoutButtonComponent);
  }  
}

